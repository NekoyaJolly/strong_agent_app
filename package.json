{
  "name": "strong_agent_app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --enable-source-maps dist/index.js",
    "dev": "tsx src/index.ts",
    "cli": "tsx src/index.ts",
    "build": "tsc -p .",
    "build:clean": "rm -rf dist && npm run build",
    "bootstrap": "sh scripts/bootstrap.sh",
    "test": "vitest",
    "test:run": "vitest run",
    "test:critical": "vitest run tests/basic.test.ts tests/config.test.ts",
    "test:unit": "vitest run tests/utils/ tests/workflow/",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "lint:ci": "eslint . --max-warnings 10",
    "lint:report": "eslint . --format html --output-file eslint-report.html",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "quality": "npm run lint:ci && npm run type-check && npm run test:run",
    "quality:strict": "npm run lint:strict && npm run type-check && npm run test:run",
    "quality:fix": "npm run lint:fix && npm run type-check && npm run test:run",
    "ci": "npm run build && npm run quality",
    "ci:strict": "npm run build && npm run quality:strict",
    "ci:full": "npm run build:clean && npm run quality && npm run test:coverage",
    "security": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "clean": "rm -rf dist coverage node_modules/.cache",
    "reset": "rm -rf node_modules package-lock.json && npm install",
    "docker:build": "docker build -t strong_agent_app .",
    "docker:run": "docker run -p 3000:3000 strong_agent_app",
    "docker:dev": "docker-compose up --build"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@openai/agents": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "openai": "^5.19.1",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.18.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-plugin-vitest": "^0.5.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4"
  },
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NekoyaJolly/strong_agent_app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NekoyaJolly/strong_agent_app/issues"
  },
  "homepage": "https://github.com/NekoyaJolly/strong_agent_app#readme"
}
