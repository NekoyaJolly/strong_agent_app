{
  "name": "strong_agent_app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --enable-source-maps dist/index.js",
    "dev": "tsx src/index.ts",
    "cli": "tsx src/index.ts",
    "build": "tsc -p .",
    "bootstrap": "sh scripts/bootstrap.sh",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "quality": "npm run lint && npm run type-check && npm run test:run",
    "ci": "npm run build && npm run quality"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@openai/agents": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "openai": "^5.19.1",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.18.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-plugin-vitest": "^0.5.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4"
  },
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NekoyaJolly/strong_agent_app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NekoyaJolly/strong_agent_app/issues"
  },
  "homepage": "https://github.com/NekoyaJolly/strong_agent_app#readme"
}
